{
  "name": "unikit",
  "version": "1.2.95",
  "description": "universal component library for react and react-native",
  "repository": "https://github.com/kations/unikit",
  "author": "kations <schoeller@kations.de> (https://github.com/kations)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kations/unikit/issues"
  },
  "homepage": "https://unikit.netlify.com",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "react-native": "src/index.js",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "unikit.podspec"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "prepare": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example/ios && node -e \"process.exit(require('os').platform() === 'darwin')\" || pod install",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "react-native-web",
    "unikit",
    "universal",
    "ios",
    "android"
  ],
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "^0.10.0",
    "@react-native-community/eslint-config": "^0.0.7",
    "@release-it/conventional-changelog": "^1.1.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.61.10",
    "babel-plugin-react-native-web": "^0.13.8",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "commitlint": "^8.3.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "~16.9.0",
    "react-native": "~0.61.5",
    "release-it": "^12.6.3",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-reanimated": "^1.7.1",
    "react-native-svg": "^12.0.3",
    "styled-components": "*"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@react-native-community"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": false,
      "addFiles": [
        "package.json"
      ],
      "commitMessage": "chore: release ${version}",
      "tagName": "v%s"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module"
    ]
  },
  "dependencies": {
    "d3-scale": "^3.2.1",
    "d3-shape": "^1.3.7",
    "dayjs": "^1.8.22",
    "deepmerge": "^4.2.2",
    "get-value": "^3.0.1",
    "prop-types": "^15.7.2",
    "set-value": "^3.0.1",
    "setimmediate": "^1.0.5",
    "svg-path-properties": "^1.0.2",
    "tinycolor2": "^1.4.1",
    "use-memo-one": "^1.1.1"
  }
}
